---
const {defaultVal, name,options =[]} = Astro.props
---
<dropdown >
    <select class="rounded-2xl w-full h-full p-1 text-pink-400 text-xs" name={name}>
        <option value="-" selected disabled >{defaultVal}</option>
        {options.map((option) => (
            <option value={option}>{option}</option>
          ))}
    </select>
</dropdown>
        

